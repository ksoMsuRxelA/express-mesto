{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 13
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": "off",
      "arrow-body-style": "off",
      "max-len": "off",
      "max-classes-per-file": "off"
    }
}
